{
    "name": "cppsarif",
    "displayName": "cppsarif",
    "description": "cppsarif",
    "icon": "share/icon.png",
    "publisher": "anonymouspc",
    "version": "1.1.6",
    "engines": {
        "vscode": "^1.100.0"
    },
    "categories": [
        "Debuggers",
        "Language Packs",
        "Themes"
    ],
    "main": "source/extension.js",
    "activationEvents": [
        "onStartupFinished"
    ],
    "contributes": {
        "configuration": {
            "title": "Sarif Directory",
            "properties": {
                "cppsarif.sarifDirectory": {
                    "type": "string",
                    "description": "Path to compiler output sarif directory. Requires: containing single or multiple .sarif files. Default: '.'",
                    "default": "."
                }
            }
        },
        "views": {
            "sarif": [
                {
                    "id": "sarifView",
                    "name": "sarifView",
                    "icon": "share/activitybar.svg"
                }
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "sarif",
                    "title": "sarif",
                    "icon": "share/activitybar.svg"
                }
            ]
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/anonymouspc/cppsarif"
    }
}
